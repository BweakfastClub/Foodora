{
    "globals": {
        "module": true,
        "require": true,
        "process": true,
        "before": true
    },
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true,
        "mocha": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "rules": {
        "no-param-reassign": ["error", { "props": false }]
    }
}
